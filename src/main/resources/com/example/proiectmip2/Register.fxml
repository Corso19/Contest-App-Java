<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proiectmip2.RegisterController">
   <children>
      <TextField fx:id="username_input" layoutX="207.0" layoutY="121.0" prefHeight="26.0" prefWidth="186.0" promptText="Insert your username here...">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </TextField>
      <Button layoutX="243.0" layoutY="347.0" mnemonicParsing="false" onAction="#onRegisterAccountClick" text="Register account" />
      <Button layoutX="523.0" layoutY="347.0" mnemonicParsing="false" text="Button" />
      <TextField fx:id="name_input" layoutX="207.0" layoutY="187.0" prefHeight="26.0" prefWidth="186.0" promptText="Insert your full name here...">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </TextField>
      <TextField fx:id="teamId_input" layoutX="207.0" layoutY="254.0" prefHeight="26.0" prefWidth="186.0" promptText="Insert your team's id here...">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </TextField>
      <Label layoutX="213.0" layoutY="40.0" text="Inregistreaza-te aici!">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
