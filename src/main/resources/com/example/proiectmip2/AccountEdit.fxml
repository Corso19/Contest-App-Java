<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proiectmip2.AccountEditController">
   <children>
      <Button layoutX="27.0" layoutY="185.0" mnemonicParsing="false" onAction="#onUpdateAccountInformationClick" text="Update account information" />
      <Button layoutX="497.0" layoutY="356.0" mnemonicParsing="false" onAction="#onReturnToMenuClick" text="Back to menu" />
      <VBox alignment="CENTER" layoutX="207.0" layoutY="22.0" prefHeight="323.0" prefWidth="200.0">
         <children>
            <Label text="Username" />
            <TextField fx:id="username_input" alignment="CENTER" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <Label text="Name" />
            <TextField fx:id="name_input" alignment="CENTER" VBox.vgrow="SOMETIMES">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <Label text="Team id" />
            <TextField fx:id="teamId_input" alignment="CENTER">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
